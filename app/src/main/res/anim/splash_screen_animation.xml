<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:anim/linear_interpolator">

    <rotate
        android:duration="700"
        android:fromDegrees="0"
        android:pivotX="50%"
        android:pivotY="50%"
        android:startOffset="0"
        android:toDegrees="35" />

    <rotate
        android:duration="700"
        android:fromDegrees="35"
        android:pivotX="50%"
        android:pivotY="50%"
        android:startOffset="1400"
        android:toDegrees="0" />

    <rotate
        android:duration="700"
        android:fromDegrees="0"
        android:pivotX="50%"
        android:pivotY="50%"
        android:startOffset="2100"
        android:toDegrees="-35" />

    <rotate
        android:duration="700"
        android:fromDegrees="-35"
        android:pivotX="50%"
        android:pivotY="50%"
        android:startOffset="2800"
        android:toDegrees="0" />
</set>
